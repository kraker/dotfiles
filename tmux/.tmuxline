# This tmux statusbar config was created by tmuxline.vim
# on Sun, 12 Sep 2021

set -g status-justify "left"
set -g status "on"
set -g status-left-style "none"
set -g message-command-style "fg=colour235,bg=colour81"
set -g status-interval "1"
set -g status-right-style "none"
set -g pane-active-border-style "fg=colour153"
set -g status-style "none,bg=colour66"
set -g message-style "fg=colour235,bg=colour81"
set -g pane-border-style "fg=colour81"
set -g status-right-length "100"
set -g status-left-length "100"
setw -g window-status-activity-style "underscore,fg=colour153,bg=colour66"
setw -g window-status-separator ""
setw -g window-status-style "none,fg=colour153,bg=colour66"
set -g status-left "#[fg=colour239,bg=colour153] [#S] #[fg=colour153,bg=colour66,nobold,nounderscore,noitalics]"
set -g status-right "#[fg=colour66,bg=colour66,nobold,nounderscore,noitalics]#[fg=colour153,bg=colour66] #(whoami) #[fg=colour81,bg=colour66,nobold,nounderscore,noitalics]#[fg=colour235,bg=colour81] #h #[fg=colour153,bg=colour81,nobold,nounderscore,noitalics]#[fg=colour239,bg=colour153] %H:%M:%S %Y-%m-%d "
setw -g window-status-format "#[fg=colour66,bg=colour66,nobold,nounderscore,noitalics]#[default] #I:#W#F #[fg=colour66,bg=colour66,nobold,nounderscore,noitalics]"
setw -g window-status-current-format "#[fg=colour66,bg=colour81,nobold,nounderscore,noitalics]#[fg=colour235,bg=colour81] #I:#W#F #[fg=colour81,bg=colour66,nobold,nounderscore,noitalics]"
